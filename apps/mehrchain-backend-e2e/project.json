{
  "name": "mehrchain-backend-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["mehrchain-backend"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "mehrchain-backend-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["mehrchain-backend:build", "mehrchain-backend:serve"]
    }
  }
}
